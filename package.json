    "node-sass-chokidar": "^0.0.3",
    "parse-gedcom": "^1.0.3",
    "prop-types": "^15.6.0",
    "react": "^16.3.0",
    "react-countup": "^3.0.2",
    "react-dom": "^16.3.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "react-tabs": "^2.2.1",
    "react-tooltip": "^3.5.0",
